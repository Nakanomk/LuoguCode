    "version": "2.0.0",
    "tasks": [
        {
            "label": "g++",
            "type": "shell",
            "command": "F:/msy/ucrt64/bin/x86_64-w64-mingw32-g++.exe",
            "args": [
                "-g",
                "${file}",
                "-o",
                "${fileDirname}\\1.exe"
            ],
            "options": {
                "cwd": "F:/msy/ucrt64/bin"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
        },
    ]
}